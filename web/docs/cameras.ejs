<% 
try{pageData=JSON.parse(pageData);}catch(err){var pageData=''}
if(pageData){
    var Title=currentBrand+' Connection URLs'; %>
    <style>
        #cameraTable tr td:first-child{vertical-align:middle}
        .table .table{background:none}
        #legend{width:100%;text-align:center;}
        #legend a{display:inline-block;text-decoration:none;}
        .body-dark #legend a{color:#fff}
    </style>
<% }else{
    var Title='Camera Brands'; %>
<style>
    #brands > div{display:inline-block;vertical-align:top}
    #brands > div ul li{padding:0}
</style>
<% } %>
<% include blocks/header.ejs %>
<body class="body-orange">
    <div class="page-wrapper">
        <!-- ******Header****** -->
        <header id="header" class="header">
            <div class="container">
                <% include blocks/branding.ejs %>
            </div><!--//container-->
        </header><!--//header-->
        <div class="doc-wrapper">
            <div class="container">
                <div id="doc-header" class="doc-header text-center">
                    <h1 class="doc-title"><%= Title %></h1>
                </div><!--//doc-header-->
                <div class="doc-body">
                    <div class="content-inner">
                        <section class="doc-section">
                            <% include blocks/search.ejs %>
                            <% if(pageData){%>
                                <% include blocks/cameras/found.ejs %>
                            <% }else{ %>
                                <% include blocks/cameras/main.ejs %>
                          <% } %>
                            <div class="text-center"><small>Camera URL information and camera brand list are from <a href="https://www.ispyconnect.com/sources.aspx">iSpyconnect</a></small></div>
                        </section><!--//content-inner-->
                    </div><!--//doc-content-->
                </div><!--//doc-body-->              
            </div><!--//container-->
        </div><!--//doc-wrapper-->
    </div><!--//page-wrapper-->
<% include blocks/footer.ejs %>
</body>
</html> 

